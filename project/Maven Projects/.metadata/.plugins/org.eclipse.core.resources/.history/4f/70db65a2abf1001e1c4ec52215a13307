package com.wipro.springcore;

public class App 
{
    public static void main( String[] args ){
    	
    	Square s = new Square();
    	s.setSide(4);
    	
    	triangle t = new triangle();
    	t.setBase(2);
    	t.setHeight(3);
    	
    	ShapeService service = new ShapeService();
    	service.setShape(s);
      
    	double area = service.findArea();
    	System.out.println(area);
    }
}
